	<script type="text/javascript">
	var toner = new L.StamenTileLayer("toner-background", {
        detectRetina: true
    });
	var map = new L.Map("map", {
	    center: new L.LatLng(55.658113792754506, 12.389278936441558),
	    zoom: 11,
	    attributionControl: false,
	    zoomControl: false
	});
	

	// Disable drag and zoom handlers.
	map.dragging.disable();
	map.touchZoom.disable();
	map.doubleClickZoom.disable();
	map.scrollWheelZoom.disable();
	map.keyboard.disable();

	L.control.attribution({position: 'topright'}).addTo(map);

	var geojsonLayer = new L.GeoJSON();
	
	map.addLayer(toner);

	var marker = new L.CircleMarker(new L.LatLng(55.658113792754506, 12.389278936441558), {
		radius: 6,
		color: 'black',
		opacity: 1.0,
		fillOpacity: 0.5
	});
	map.addLayer(marker);				
	marker.bindPopup('<p class="lastseen">Last seen here.</p>');
		
</script>
